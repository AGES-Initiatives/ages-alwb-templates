	Section Basil
		when-date-is
			Jan 1 use:
				Insert_section Selector_Trisagion.holygod End-Insert
			Jan 6 use:
				when-name-of-day-is
					Sunday, Monday use:
						Insert_section Selector_Trisagion.baptized End-Insert
					otherwise use:
						Insert_section Selector_Trisagion.holygod End-Insert
				end-when
			Dec 25 use:
				when-name-of-day-is
					Sunday, Monday use:
						Insert_section Selector_Trisagion.baptized End-Insert
					otherwise use:
						Insert_section Selector_Trisagion.holygod End-Insert
				end-when
			otherwise use:
				when-movable-cycle-day-is
					29, 36, 50, 57, 69 use:
						Insert_section Selector_Trisagion.holygod End-Insert
					43 use:
						Insert_section Selector_Trisagion.cross End-Insert
					71 use:
						Insert_section Selector_Trisagion.baptized End-Insert
				end-when
		end-when
	End-Section

