	Section _01_Enarxis__
		Section daily
			when-movable-cycle-day-is
				71, 72, 73, 74, 75, 76, 77, 109 use:
					Section pascha
						Para role typikon<b>sid tyPart.Enarxis</>sid pr.enarxis02.text End-Para
						Para role typikonindent sid pe.d071_gr_GR_cog.peMA.Troparion.incipit<r>sid tyPE.PaschalEnarxis1</>End-Para
					End-Section
				78 thru 108 use:
					Section afterpascha
						Para role typikon<b>sid tyPart.Enarxis</>sid pr.enarxis02.text End-Para
						Para role typikonindent sid pe.d071_gr_GR_cog.peMA.Troparion.incipit<r>sid tyPE.PaschalEnarxis2</> sid pr.ComeLetUsWorship1.incipit End-Para
					End-Section
				otherwise use:
					Para role typikon<b>sid tyPart.Enarxis</>sid pr.enarxis02.text End-Para
			end-when	
			Section _02
				Para role typikonindent<r>sid tyPart.GreatLitany</>End-Para
			End-Section
		End-Section
		Section vesperal
			Dialog sid pr.enarxis02.text End-Dialog
			Insert_section VE._01_Beginning__.vespers.introductorypsalm End-Insert
		End-Section
	End-Section

