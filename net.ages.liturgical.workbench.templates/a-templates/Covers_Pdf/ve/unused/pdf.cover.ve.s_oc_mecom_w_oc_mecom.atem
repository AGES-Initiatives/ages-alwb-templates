Template pdf.cover.ve.s_oc_mecom_w_oc_mecom

Status NA

	import da.*
	import oc.*
	import me.*

	import pdf.covers_en_US_goarch.*

	import actors_gr_GR_cog.ac.*
	import miscellanea_gr_GR_cog.misc.*
	import prayers_gr_GR_cog.*
	import rubrical_gr_GR_cog.*
	import template.titles_gr_GR_cog.*
	import titles_gr_GR_cog.*

	import iTags.*
	import bTags.*
	import roles.*
	import rules.*
	
	Switch-Version L2 End-Switch-Version

		Section title
			Para role logo1 sid logo1.text End-Para
			Para role logo2 sid logo2.text End-Para
			Title role cover1 sid ve.pdf.cover End-Title
			Insert_section pdf.cover.lookup.on_evening_before End-Insert
			Title role cover3 sid d.for ldp @Service_Date End-Title 	
		End-Section
		Section modeofweek
			Title role cover5 sid book.Octoechos.name sid d.delimiter rid ocBK.ModeNaked End-Title
			Insert_section pdf.cover.lookup.on_evening_before End-Insert
		End-Section
		Section commeoration
			when-name-of-day-is
				Sunday use:
					Insert_section pdf.cover.lookup.when_sunday_dates End-Insert
				otherwise use:
					when-date-is Dec 19 use:
							when-name-of-day-is 
								Saturday use:
									Title role cover5 sid book.Menaion.name sid d.delimiter sid me.m12.d19_gr_GR_cog.meDA.date End-Title
									Title role cover4 sid me.m12.d19_gr_GR_cog.meDA.commemoration.first_short End-Title
									Title role cover5 sid book.Menaion.name sid d.delimiter sid me.m12.d20_gr_GR_cog.meDA.date End-Title
									Title role cover4 sid me.m12.d20_gr_GR_cog.meDA.commemoration.second_short End-Title
								otherwise use:
									Title role cover5 sid book.Menaion.name sid d.delimiter sid me.m12.d19_gr_GR_cog.meDA.date End-Title
									Title role cover4 sid me.m12.d19_gr_GR_cog.meDA.commemoration.first_short End-Title
							end-when
					end-when		
			end-when
		End-Section
		
		Title role cover6 sid pref.generation_alwb.cover.version End-Title
	
	Switch-Version Both End-Switch-Version	
	
End-Template