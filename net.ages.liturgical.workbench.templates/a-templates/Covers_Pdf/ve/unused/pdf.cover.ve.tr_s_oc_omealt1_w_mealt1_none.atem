Template pdf.cover.ve.tr_s_oc_omealt1_w_mealt1_none

Status NA

	import da.*
	import oc.*
	import me.*
	import eo.*
	import tr.*
	import pe.*
	
	import he.k_en_US_public.*
	import pdf.covers_en_US_goarch.*

	import actors_gr_GR_cog.ac.*
	import misc_gr_GR_cog.*
	import prayers_gr_GR_cog.*
	import rubrical_gr_GR_cog.*
	import template.titles_gr_GR_cog.*
	import titles_gr_GR_cog.*

	import iTags.*
	import bTags.*
	import roles.*
	import rules.*
	
	Switch-Version L2 End-Switch-Version
	

		Section title
			Para role logo1 sid logo1.text End-Para
			Para role logo2 sid logo2.text End-Para
			Title role cover1 sid ve.pdf.cover End-Title
			Insert_section pdf.cover.lookup.on_evening_before End-Insert
			Title role cover3 sid d.for ldp @Service_Date End-Title 	
		End-Section

		when-movable-cycle-day-is
			128 thru 365 use:
				Section normal
					Section modeofweek
						when-name-of-day-is
							Sunday use:
								Title role cover5 sid book.Octoechos.name sid d.delimiter rid ocBK.ModeNaked End-Title
								Insert_section pdf.cover.lookup.on_evening_before End-Insert
						end-when
					End-Section
					Section commeoration
						Title role cover5 sid book.MeSupplement.name sid d.delimiter rid alt1.meDA.date End-Title
						Title role cover4 rid alt1.meDA.commemoration.first_short End-Title
					End-Section
				End-Section
				
			1 thru 22 use:
				Section triodion
					Section modeofweek
						when-name-of-day-is
							Sunday use:
								Title role cover5 sid book.Octoechos.name sid d.delimiter rid ocBK.ModeNaked End-Title
								Insert_section pdf.cover.lookup.on_evening_before End-Insert
						end-when
					End-Section
					Section moveable
						Insert_section pdf.cover.lookup.mc_vespers_on_day_before End-Insert
					End-Section
					Section commeoration
						when-movable-cycle-day-is
							1, 8, 14, 15, 21, 22 use:
							otherwise use:
								Title role cover5 sid book.MeSupplement.name sid d.delimiter rid alt1.meDA.date End-Title
								Title role cover4 rid alt1.meDA.commemoration.first_short End-Title
						end-when
					End-Section
				End-Section

			23 thru 65 use:
				Section lent
					Section modeofweek
						Insert_section pdf.cover.lookup.tr_modeofweek_ve End-Insert
					End-Section
					Section moveable
						Insert_section pdf.cover.lookup.mc_vespers_on_day_before End-Insert
					End-Section
					Section commeoration
						when-movable-cycle-day-is
							28, 29, 36, 43, 50, 54, 56, 57, 63, 64 use:
							otherwise use:
								Title role cover5 sid book.Menaion.name sid d.delimiter rid meDA.date End-Title
								Title role cover4 rid meDA.commemoration.first_short End-Title
						end-when
					End-Section
				End-Section
		end-when

		Title role cover6 sid pref.generation_alwb.cover.version End-Title
	
	Switch-Version Both End-Switch-Version	
	
End-Template