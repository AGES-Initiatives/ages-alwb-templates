	Head 
	//	Set_Date month 3 day 3 year 2018 End_Date
			Page_Header_Even
				center @lookup sid h1.pdf.header sid d.on lang L1 @date lang L1
			End_Page_Header_Even
			Page_Header_Odd
				center @lookup sid h1.pdf.header sid d.on lang L2 @date lang L2
