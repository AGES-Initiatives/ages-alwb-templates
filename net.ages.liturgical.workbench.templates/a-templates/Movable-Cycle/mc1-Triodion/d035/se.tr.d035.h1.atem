Template se.tr.d035.h1

Status NA

	import da.*
	import oc.*
	import me.*
	import eo.*
	import tr.*
	import pe.*

	import actors_gr_GR_cog.*
	import dismissals_gr_GR_cog.*
	import miscellanea_gr_GR_cog.*
	import prayers_gr_GR_cog.*
	import rubrical_gr_GR_cog.*
	import template.titles_gr_GR_cog.*
	import titles_gr_GR_cog.*

	import iTags.*
	import bTags.*
	import roles.*

	Head
	//	Set_Date month 3 day 3 End_Date
			Page_Header_Even
				center @lookup sid template.titles_gr_GR_cog.h1.pdf.header lang L1 @lookup rid daOnDay.text lang L1 @lookup rid meDA.pdfheaderdate lang L1
			End_Page_Header_Even
			Page_Header_Odd
				center @lookup sid template.titles_gr_GR_cog.h1.pdf.header lang L2 @lookup rid daOnDay.text lang L2 @lookup rid meDA.pdfheaderdate lang L2
			End_Page_Header_Odd
			Page_Footer_Even
				left @pageNbr
				right @lookup sid pdf.covers_en_US_goarch.footer.attribution.text lang L2
			End_Page_Footer_Even
			Page_Footer_Odd
				left @lookup sid pdf.covers_en_US_goarch.footer.attribution.text lang L2
				right @pageNbr
			End_Page_Footer_Odd
		Set_Page_Number 1 End_Set_Page_Number
	End_Head

	Section Instance01
		Insert_section HR.h1__.beginning End-Insert
		when-date-is
			Mar 26 use:
				Insert_section AP.NM.af End-Insert
			otherwise use:
				Insert_section AP.NM.tr1_ End-Insert
		end-when
		Insert_section HR.h1__.middle End-Insert
		Section kontakion
			when-date-is
				Mar 26 use:
					Insert_section KO.NM.af End-Insert
				otherwise use:
					Title role desig sid ti.Kontakion End-Title
					Title role mode sid he.a.m8_gr_GR_cog.heAU.OsAparchas.mode End-Title
					Hymn sid he.a.m8_gr_GR_cog.heAU.OsAparchas.text @ver media-off End-Hymn
			end-when
		End-Section
		Insert_section HR.h1__.end_nocommemoration End-Insert
	End-Section

End-Template