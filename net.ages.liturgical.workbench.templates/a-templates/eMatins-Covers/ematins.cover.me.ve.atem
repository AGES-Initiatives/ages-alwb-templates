Template ematins.cover.me.ve

Status NA

	import da.*
	import oc.*
	import me.*
	import eo.*
	import tr.*
	import pe.*
	
	import he.k_en_US_public.*
	import ematinscovers_en_US_ematins.*

	import actors_gr_GR_cog.*
	import misc_gr_GR_cog.*
	import prayers_gr_GR_cog.*
	import rubrical_gr_GR_cog.*
	import template.titles_gr_GR_cog.*
	import titles_gr_GR_cog.*

	import iTags.*
	import bTags.*
	import roles.*
	import rules.*
	
//	Head
//		Set_Date month 10 day 15 End_Date
//	End_Head

	Switch-Version L2 End-Switch-Version

		Section title
			Title role cover0 sid logo.text End-Title
			Title role cover1 sid ve.pdf.cover End-Title 	//Service
			Title role cover3 sid d.ontheeveof ldp @Service_Date End-Title 	
		End-Section
		Section commeoration
			when-name-of-day-is
				Sunday use:
					when-date-is
						Jul 13 thru 19 use:
							Title role cover4 sid me.m07.dHF_gr_GR_cog.meCommemoration.text End-Title
						Oct 11 thru 17 use:
							Title role cover4 sid me.m10.dHF_gr_GR_cog.meCommemoration.text End-Title
						Dec 11 thru 17 use:
							Title role cover4 sid me.m12.dFF_gr_GR_cog.meCommemoration.text End-Title
						Dec 18 thru 24 use:
							Title role cover4 sid me.m12.dBC_gr_GR_cog.meCommemoration.text End-Title
						Dec 26 thru 31 use:
							Title role cover4 sid me.m12.dAC_gr_GR_cog.meCommemoration.text End-Title
					end-when
				otherwise use: 
					when-date-is
						Dec 17 use: 
							Title role cover4 rid alt1.meShortCommemoration1.text End-Title
						otherwise use: 
							Title role cover4 rid meCommemoration.text End-Title
					end-when
			end-when
		End-Section
		Section sudnay_mode
			when-name-of-day-is 
				Sunday use: 
					Title role cover4 sid Anastasima rid ocVE.Apolytikion.mode End-Title
			end-when
		End-Section
		Title role cover5 sid pref.generation_public.cover.version End-Title //GE or E
	
	Switch-Version Both End-Switch-Version	
	
End-Template