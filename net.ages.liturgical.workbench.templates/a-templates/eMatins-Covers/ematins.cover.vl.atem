Template ematins.cover.vl

Status NA

	import da.*
	import oc.*
	import me.*
	import eo.*
	import tr.*
	import pe.*
	
	import he.k_en_US_public.*
	import ematinscovers_en_US_ematins.*

	import actors_gr_GR_cog.*
	import misc_gr_GR_cog.*
	import prayers_gr_GR_cog.*
	import rubrical_gr_GR_cog.*
	import template.titles_gr_GR_cog.*
	import titles_gr_GR_cog.*

	import iTags.*
	import bTags.*
	import roles.*
	import rules.*
	
	Switch-Version L2 End-Switch-Version
	

	when-movable-cycle-day-is 
	69 use:
		Section hw_vl1
			Title role cover0 sid logo.text End-Title
			Title role cover1 sid hw.pdf.cover ldp @Service_Year End-Title 	//Service
			Title role cover2 sid vl.pdf.cover sid d.onThursdayMorning End-Title
			Title role cover4 sid ve.pdf.header sid d.of rid trShortCommemoration1.text End-Title	//Triodion
		End-Section //hw_vl1

	71 use:
		Section hw_vl2
			Title role cover0 sid logo.text End-Title
			Title role cover1 sid hw.pdf.cover ldp @Service_Year End-Title 	//Service
			Title role cover2 sid vl.pdf.cover sid d.onSaturdayMorning End-Title
			Title role cover4 sid ve.pdf.header sid d.of rid peShortCommemoration1.text End-Title	
		End-Section //hw_vl2

		otherwise use:
			when-date-is
				Dec 25 use:
					Title role cover0 sid logo.text End-Title
					Title role cover1 sid vl.pdf.cover End-Title 	//Service
					Title role cover3 sid me.m12.d25.onChristmasEve End-Title 	
				
				Jan 6 use:
					Title role cover0 sid logo.text End-Title
					Title role cover1 sid vl.pdf.cover End-Title 	//Service
					Title role cover3 sid me.m01.d06.onEpiphanyEve End-Title 	
			end-when
	end-when
	
	Title role cover5 sid pref.generation_public.cover.version End-Title //GE or E
	
	Switch-Version Both End-Switch-Version	
	
End-Template