//	Head
//		Set_Date month 10 day 11 End_Date
//	End_Head
	
	Switch-Version L2 End-Switch-Version

		Section title
			Title role cover0 sid logo.text End-Title
			Title role cover1 sid ma.pdf.cover End-Title
			Title role cover3 sid d.for ldp @Service_Date End-Title
		End-Section 

		when-movable-cycle-day-is
			128 thru 365 use:
				Section normal
					Section mode_eothinon
						when-name-of-day-is 
							Sunday use: 
								Title role cover5 sid book.Octoechos.name sid d.delimiter rid ocVE.Stichera1.mode End-Title
								Insert_section ematins.pdf.cover.lookup.on_morning End-Insert
								Title role cover4 sid Eothinon ldp @Eothinon End-Title
						end-when
					End-Section
					Section commeoration
						when-name-of-day-is
							Sunday use:
								Insert_section ematins.pdf.cover.lookup.when_sunday_dates End-Insert
							otherwise use:
								Title role cover5 sid book.Menaion.name sid d.delimiter rid meDate.text End-Title
								Title role cover4 rid meCommemoration.text End-Title
						end-when
					End-Section
					Section katavasia
						when-name-of-day-is
							Sunday use: Insert_section KA._title End-Insert
						end-when
					End-Section
				End-Section
	
			1 thru 63 use:
				Section triodion_lent
					Section mode_eothinon
						when-name-of-day-is 
							Sunday use: 
								Title role cover5 sid book.Octoechos.name sid d.delimiter rid ocVE.Stichera1.mode End-Title
								Insert_section ematins.pdf.cover.lookup.on_morning End-Insert
								Title role cover4 sid Eothinon ldp @Eothinon End-Title
						end-when
					End-Section
					Section moveable
						when-movable-cycle-day-is
							1, 8, 14, 15, 21, 22, 28, 29, 36, 43, 50, 57 use:
								Insert_section ematins.pdf.cover.lookup.mc_matins End-Insert
						end-when
					End-Section
					Section commeoration
						when-movable-cycle-day-is
							1, 8, 15, 21, 22, 28, 29, 36, 43, 50, 57 use:
								Insert_section ematins.pdf.cover.lookup.when_sunday_dates End-Insert
							14 use:
							otherwise use:
								Title role cover5 sid book.Menaion.name sid d.delimiter rid meDate.text End-Title
								Title role cover4 rid meShortCommemoration1.text End-Title
						end-when
					End-Section
					Section katavasia
						when-movable-cycle-day-is
							1, 8, 15, 21, 22, 28, 29, 36, 43, 50, 57 use:
								Insert_section KA._title End-Insert
						end-when
					End-Section
				End-Section
		end-when

		Title role cover6 sid pref.generation_alwb.cover.version End-Title
		
	Switch-Version Both End-Switch-Version	

End-Template