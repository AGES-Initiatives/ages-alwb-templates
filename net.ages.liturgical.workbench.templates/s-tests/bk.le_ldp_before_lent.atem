	Head
		Set_Date month 2 day 1 year 2026 End_Date
			Page_Header_Even
				center @lookup sid hw.bridegroomservice lang L1 @lookup sid d.onSundayEvening lang L1
			End_Page_Header_Even
			Page_Header_Odd
				center @lookup sid hw.bridegroomservice lang L2 @lookup sid d.onSundayEvening lang L2
			End_Page_Header_Odd
			Page_Footer_Even
				center @pageNbr
			End_Page_Footer_Even
			Page_Footer_Odd
				center @pageNbr
			End_Page_Footer_Odd
		Set_Page_Number 1 End_Set_Page_Number
	End_Head

	Section test_mc
		Set_Date month 1 day 31 year 2026 End_Date
		Para ldp @Day_of_Movable_Cycle End-Para
		when-movable-cycle-day-is
			0 use:
				Para ldp @Day_of_Movable_Cycle End-Para		
				Para rid meDA.date End-Para
			1 use:
				Para rid meDA.date End-Para
			357 use:
				Para rid meDA.date End-Para
		end-when
	End-Section
