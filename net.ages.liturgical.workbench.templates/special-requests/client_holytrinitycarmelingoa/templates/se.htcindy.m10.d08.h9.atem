Template se.htcindy.m10.d08.h9

Status NA

	import eu.consecration_gr_GR_cog.*
	
	import actors_gr_GR_cog.*
	import client_gr_GR_cog.*
	import dismissals_gr_GR_cog.*
	import miscellanea_gr_GR_cog.*
	import prayers_gr_GR_cog.*
	import rubrical_gr_GR_cog.*
	import template.titles_gr_GR_cog.*
	import titles_gr_GR_cog.*
		
	import iTags.*
	import bTags.*
	import roles.*

	Head 
		Set_Date month 10 day 7 year 2023 End_Date
			Page_Header_Even
				center @lookup sid ve.pdf.header rid da.daVE.OnTheEveningBefore.text lang L1 
			End_Page_Header_Even
			Page_Header_Odd
				center @lookup sid ve.pdf.header rid da.daVE.OnTheEveningBefore.text lang L2
			End_Page_Header_Odd
			Page_Footer_Even
				center @pageNbr
			End_Page_Footer_Even
			Page_Footer_Odd
				center @pageNbr
			End_Page_Footer_Odd
		Set_Page_Number 1 End_Set_Page_Number
	End_Head

//	Preface Covers
//		Insert_template pdf.cover.ve.override End-Insert
//		Insert_template pdf.cover.credits End-Insert
//	End-Preface

	Actor sid ac.Choir End-Actor
	
	// hymns of saints of relics
	Title role desig sid cl.relics.saint1.Apolytikion.title End-Title
	Title role mixed <Tmode>sid cl.relics.saint1.Apolytikion.mode</> <Tmelody>sid cl.relics.saint1.Apolytikion.melody</> End-Title
	Hymn sid cl.relics.saint1.Apolytikion.text @ver End-Hymn

	Title role desig sid cl.relics.saint2.Apolytikion.title End-Title
	Title role mixed <Tmode>sid cl.relics.saint2.Apolytikion.mode</> <Tmelody>sid cl.relics.saint2.Apolytikion.melody</> End-Title
	Hymn sid cl.relics.saint2.Apolytikion.text @ver End-Hymn

	Title role desig sid cl.relics.saint3.Apolytikion.title End-Title
	Title role mixed <Tmode>sid cl.relics.saint3.Apolytikion.mode</> <Tmelody>sid cl.relics.saint3.Apolytikion.melody</> End-Title
	Hymn sid cl.relics.saint3.Apolytikion.text @ver End-Hymn
	
	//hymn of local church
	Title role desig sid cl.Apolytikion1.title End-Title
	Title role mixed <Tmode> sid cl.Apolytikion1.mode</> <Tmelody> sid cl.Apolytikion1.melody</>End-Title
	Hymn sid cl.Apolytikion1.text @ver End-Hymn

	Title role mode sid euCON.Key1003.mode End-Title
	Verse sid pr.GloryToTheFather.text End-Verse
	Hymn sid euCON.Key1003.text @ver End-Hymn
	
	Title role mode sid euCON.Key1004.mode End-Title
	Verse sid pr.BothNowAndEver.text End-Verse
	Hymn sid euCON.Key1004.text @ver End-Hymn

