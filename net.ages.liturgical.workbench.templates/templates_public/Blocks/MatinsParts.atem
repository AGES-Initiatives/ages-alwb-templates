		when-mode-of-week-is 
			M1 use:
				when-triodion-day-is 43 use:
					when-exists rid ocMA.Kathisma13.melody use:
						Title role melody rid ocMA.Kathisma13.melody End-Title
					end-when
					Hymn rid ocMA.Kathisma13.text End-Hymn
					otherwise use:
						when-exists rid ocVE.ApolTheotokionVM.melody use:
							Title role mixed<Tmelody>rid ocVE.ApolTheotokionVM.melody</>End-Title
						end-when
						Hymn rid ocVE.ApolTheotokionVM.text End-Hymn
				end-when
			M2 use:
				when-triodion-day-is 29 use:
					when-exists rid ocMA.Kathisma13.melody use:
						Title role melody rid ocMA.Kathisma13.melody End-Title
					end-when
					Hymn rid ocMA.Kathisma13.text End-Hymn
					otherwise use:
						when-exists rid ocVE.ApolTheotokionVM.melody use:
							Title role mixed<Tmelody>rid ocVE.ApolTheotokionVM.melody</>End-Title
						end-when
						Hymn rid ocVE.ApolTheotokionVM.text End-Hymn
				end-when
			M3, M4, M5, M6, M7 use:
				when-exists rid ocVE.ApolTheotokionVM.melody use:
					Title role mixed<Tmelody>rid ocVE.ApolTheotokionVM.melody</>End-Title
				end-when
				Hymn rid ocVE.ApolTheotokionVM.text End-Hymn
			M8 use:
				when-triodion-day-is 36, 50, 57 use:
					when-exists rid ocMA.Kathisma13.melody use:
						Title role melody rid ocMA.Kathisma13.melody End-Title
					end-when
					Hymn rid ocMA.Kathisma13.text End-Hymn
					otherwise use:
						when-exists rid ocVE.ApolTheotokionVM.melody use:
							Title role mixed<Tmelody>rid ocVE.ApolTheotokionVM.melody</>End-Title
						end-when
						Hymn rid ocVE.ApolTheotokionVM.text End-Hymn
				end-when
		end-when			
