	Section Basil
		Insert_section prayer End-Insert
		when-date-is
			Jan 1 use:
				Insert_section holygod End-Insert
			Jan 6 use:
				when-name-of-day-is
					Sunday, Monday use:
						Insert_section baptized End-Insert
					otherwise use:
						Insert_section holygod End-Insert
				end-when
			Dec 25 use:
				when-name-of-day-is
					Sunday, Monday use:
						Insert_section baptized End-Insert
					otherwise use:
						Insert_section holygod End-Insert
				end-when
			otherwise use:
				when-movable-cycle-day-is
					29, 36, 50, 57, 69 use:
						Insert_section holygod End-Insert
					43 use:
						Insert_section cross End-Insert
				end-when
		end-when		
	End-Section	

	Section Chrysostom
		Insert_section prayer End-Insert
		when-date-is
			Jan 6, 7 use:
				Insert_section baptized End-Insert
			Aug 1 use:
				Insert_section cross End-Insert
			Sep 14 use:
				Insert_section cross End-Insert
			Dec 25, 26 use:
				Insert_section baptized End-Insert
			otherwise use:
				when-movable-cycle-day-is
					63, 71, 72, 73, 74, 75, 76, 77, 109, 120, 121 use:
						Insert_section baptized End-Insert
					otherwise use:
						Insert_section holygod End-Insert
				end-when
		end-when
	End-Section	

