Template rid_EpistleProkeimena_MovableCycle

Status Draft

/* 
 These section compute the Epistle title and prokeimenon for pentecostarion commemorations.
 * The source is the book template in Apostolos.

*/

	Section mc_ep_title_mode
		when-movable-cycle-day-is
			071 use: Insert_section bk.ap.mc.p0.Day_Sunday01.ep_title End-Insert

			110 use: Insert_section bk.ap.mc.p0.Week_06.Day_Thursday.ep_title End-Insert
			113 use: Insert_section bk.ap.mc.p0.Day_Sunday07.ep_title End-Insert
			119 use: Insert_section bk.ap.mc.p0.Week_07.Day_Saturday.ep_title End-Insert
			120 use: Insert_section bk.ap.mc.p0.Day_Sunday08.ep_title End-Insert
			121 use: Insert_section bk.ap.mc.p1.Week_01.Day_Monday.ep_title End-Insert
			122 use: Insert_section bk.ap.mc.p1.Week_01.Day_Tuesday.ep_title End-Insert
			127 use: Insert_section bk.ap.mc.p1.Day_Sunday_01.ep_title End-Insert


		end-when
	End-Section



	Section mc_ep_prokeimenon_verse
		when-movable-cycle-day-is
			071 use: Insert_section bk.ap.mc.p0.Day_Sunday01.ep_prokeimenon End-Insert

			110 use: Insert_section bk.ap.mc.p0.Week_06.Day_Thursday.ep_prokeimenon End-Insert
			113 use: Insert_section bk.ap.mc.p0.Day_Sunday07.ep_prokeimenon End-Insert
			119 use: Insert_section bk.ap.mc.p0.Week_07.Day_Saturday.ep_prokeimenon End-Insert
			120 use: Insert_section bk.ap.mc.p0.Day_Sunday08.ep_prokeimenon End-Insert
			121 use: Insert_section bk.ap.mc.p1.Week_01.Day_Monday.ep_prokeimenon End-Insert
			122 use: Insert_section bk.ap.mc.p1.Week_01.Day_Tuesday.ep_prokeimenon End-Insert
			127 use: Insert_section bk.ap.mc.p1.Day_Sunday_01.ep_prokeimenon End-Insert

		end-when
	End-Section


End-Template



