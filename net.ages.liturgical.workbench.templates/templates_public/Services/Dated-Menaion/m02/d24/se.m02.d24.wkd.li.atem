Template se.m02.d24.wkd.li
//weekday liturgy for celebrated saint

	import client_gr_GR_cog.*
	import eu.lichrysbasil_gr_GR_cog.*
	import ho.s07_gr_GR_cog.*
	import me.m02.d24_gr_GR_cog.*
	
	import actors_gr_GR_cog.*
	import misc_gr_GR_cog.*
	import prayers_gr_GR_cog.*
	import rubrical_gr_GR_cog.*
	import titles_gr_GR_cog.*
		
	import iTags.*
	import bTags.*
	import roles.*
	
	//Imported Template

	Head
		Set_Date month 2 day 24 End_Date
	End_Head

	Insert_section LI._01_Enarxis__.daily End-Insert
	Insert_section LI._02_Antiphons__.celebrated_saint End-Insert

	Section Part_04_2_Antiphon3 //festal antiphons
		Actor sid Choir End-Actor
		Title role desig sid Antifonon3 End-Title
		Title role mixed<Tmode>sid meVE.Apolytikion1.mode</><Tmelody>sid meVE.Apolytikion1.melody</>End-Title
		Hymn sid meVE.Apolytikion1.text End-Hymn
	End-Section

	Insert_section LI._04_Entrance__.clergy End-Insert
	
	Insert_section LI._05_AfterEntranceHymns__.choir End-Insert
	Title role desig sid Apolytikion End-Title
		Title role mixed<Tmode>sid meVE.Apolytikion1.mode</><Tmelody>sid meVE.Apolytikion1.melody</>End-Title
	Hymn sid meVE.Apolytikion1.text End-Hymn

	Insert_section LI._05_AfterEntranceHymns__.local End-Insert
	Insert_section LI._05_AfterEntranceHymns__.kontakion End-Insert
Insert_section LI._06_Trisagion__.chrys  End-Insert
	Insert_section LE._01_Epistle__.menaion End-Insert
	Insert_section LE._02_Gospel__.menaion_ End-Insert
	Insert_section LI._08_Liturgy__.chrys End-Insert
	
	Section Part_15_1_CommunionHymn
		Actor sid Choir End-Actor
		Title role desig sid Koinonikon End-Title
		// The following communion hymn may change on a feast
		Hymn sid meLI.CommunionHymn.text End-Hymn
	End-Section
	
	Insert_section LI._09_Dismissal__.commemoration End-Insert

End-Template
