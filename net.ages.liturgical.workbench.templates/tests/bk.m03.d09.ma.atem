Template bk.m03.d09.ma

Status NA

	import me.*
	
	import actors_gr_GR_cog.*
	import dismissals_gr_GR_cog.*
	import misc_gr_GR_cog.*
	import prayers_gr_GR_cog.*
	import rubrical_gr_GR_cog.*
	import template.titles_gr_GR_cog.*
	import titles_gr_GR_cog.*
		
	import iTags.*
	import bTags.*
	import roles.*

	Head 
		Set_Date month 3 day 9 year 2018 End_Date
			Page_Header_Even
				center @lookup sid ma.pdf.header sid d.on lang L2 @date lang L2
			End_Page_Header_Even
			Page_Header_Odd
				center @lookup sid ma.pdf.header sid d.on lang L2 @date lang L2
			End_Page_Header_Odd
			Page_Footer_Even
				center @pageNbr
			End_Page_Footer_Even
			Page_Footer_Odd
				center @pageNbr
			End_Page_Footer_Odd
		Set_Page_Number 1 End_Set_Page_Number
	End_Head

	Insert_section MA._12_Exaposteilarion__.me__.exap123t End-Insert

	Section theotokion
		when-exists
			rid meMA.ExapTheotokion.text use:
				when-name-of-day-is
					Monday, Tuesday, Thursday, Saturday, Sunday use:
						Section exaptheotokion
							Title role desig sid Theotokion End-Title
							Title<Tmode>rid meMA.ExapTheotokion.mode</><Tmelody>rid meMA.ExapTheotokion.melody</>End-Title
							Hymn rid meMA.ExapTheotokion.text @ver End-Hymn
						End-Section
					Wednesday, Friday use:
						when-exists rid meMA.ExapStavrotheotokion.text use:
							Section exapstavrotheotokion
								Title role desig sid Stavrotheotokion End-Title
								Title<Tmode>rid meMA.ExapStavrotheotokion.mode</><Tmelody>rid meMA.ExapStavrotheotokion.melody</>End-Title
								Hymn rid meMA.ExapStavrotheotokion.text @ver End-Hymn
							End-Section
						otherwise use:
							when-exists rid meMA.ExapTheotokion.text use:
								Insert_section exaptheotokion End-Insert
							end-when
						end-when
				end-when
		end-when
	End-Section

End-Template
