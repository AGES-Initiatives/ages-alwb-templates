Template bk.test01 

	import client_gr_GR_cog.*
	import ho.s10_gr_GR_cog.*	

	import da.*
	import me.*
	import oc.*
	import pe.*
	import tr.*

	//Templates
	import bk.ho.s15_m05.*
	
	import actors_gr_GR_cog.*
	import dismissals_gr_GR_cog.*
	import misc_gr_GR_cog.*
	import prayers_gr_GR_cog.*
	import rubrical_gr_GR_cog.*
	import template.titles_gr_GR_cog.*
	import titles_gr_GR_cog.*
		
	import iTags.*
	import bTags.*
	import roles.*

	Head 
		Set_Date month 1 day 11 year 2015 End_Date
			Page_Header_Even
				center @lookup sid ve.pdf.header rid daVE.OnTheEveningBefore lang L2 
			End_Page_Header_Even
			Page_Header_Odd
				center @lookup sid ve.pdf.header rid daVE.OnTheEveningBefore lang L2
			End_Page_Header_Odd
			Page_Footer_Even
				center @pageNbr
			End_Page_Footer_Even
			Page_Footer_Odd
				center @pageNbr
			End_Page_Footer_Odd
		Set_Page_Number 1 End_Set_Page_Number
	End_Head

	Preface Covers
		Insert_template ematins.cover.ve End-Insert
		Insert_template ematins.credits End-Insert
	End-Preface
	

	Para ldp@Day_of_Movable_Cycle End-Para


	Section Instance01 //feast day
		Title sid Doxa End-Title
		
		Section feast		
			when-date-is
				Jan 1, 6 use:
					Insert_section AP.me1_ End-Insert
				Jan 7 thru 14 use:
					Insert_section AP.af End-Insert
					Insert_section AP.me1_ End-Insert
				Feb 2 use:
					Insert_section AP.me1_ End-Insert
				Feb 3 thru 9 use:
					Insert_section AP.af End-Insert
					Insert_section AP.me1_ End-Insert
				Mar 25 use:
					Insert_section AP.me1_ End-Insert
				otherwise use:
					when-movable-cycle-day-is
						78 use:
							Insert_section AP.pe1_ End-Insert
						79 thru 84 use:
							Insert_section AP.af End-Insert
							Insert_section AP.me1_ End-Insert
						95 use:
							Insert_section AP.pe1_ End-Insert
						96 thru 102 use:
							Insert_section AP.af End-Insert
							Insert_section AP.me1_ End-Insert
						110 use:
							Insert_section AP.pe1_ End-Insert
						111 thru 118 use:
							Insert_section AP.af End-Insert
							Insert_section AP.me1_ End-Insert
						120, 121 use:
							Insert_section AP.pe1_ End-Insert
						122 thru 126 use:
							Insert_section AP.af End-Insert
							Insert_section AP.me1_ End-Insert
						otherwise use:
							when-date-is
								Aug 6, 15 use:
									Insert_section AP.me1_ End-Insert
								Aug 7, 8, 9, 10, 11, 12, 13, 16, 17, 18, 19, 20, 21, 22, 23 use:
									Insert_section AP.af End-Insert
									Insert_section AP.me1_ End-Insert
								Sep 8, 14 use:
									Insert_section AP.me1_ End-Insert
								Sep 9, 10, 11, 12, 15, 16, 17, 18, 19, 20, 21 use:
									Insert_section AP.af End-Insert
									Insert_section AP.me1_ End-Insert
								Nov 21 use:
									Insert_section AP.me1_ End-Insert
								Nov 22, 23, 24, 25 use:
									Insert_section AP.af End-Insert
									Insert_section AP.me1_ End-Insert
								Dec 25, 26 use:
									Insert_section AP.me1_ End-Insert
								Dec 27, 28, 29, 30, 31 use:
									Insert_section AP.af End-Insert
									Insert_section AP.me1_ End-Insert
								otherwise use:
									Insert_section AP.me1_ End-Insert
							end-when
					end-when
			end-when
		End-Section	




	End-Section





















End-Template